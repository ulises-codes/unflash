{
  "name": "UnFlash",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_start",
      "js": ["hidePageOnLoad.js"]
    },
    {
      "matches": ["<all_urls>"],
      "run_at": "document_end",
      "js": ["stop.js"]
    }
  ]
}
